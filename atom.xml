---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Scriptus news</title>
 <link href="http://ianso.github.com/scriptus/atom.xml" rel="self"/>
 <link href="http://ianso.github.com/scriptus/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://ianso.github.com/scriptus/</id>
 <author>
   <name>Ian Sollars</name>
   <email>ian.sollars@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://ianso.github.com/scriptus/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://ianso.github.com/scriptus{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
